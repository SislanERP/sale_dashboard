<p id="notice"><%= notice %></p>

<h1>Listing Type Docs</h1>

<table>
  <thead>
    <tr>
      <th>Cod sii</th>
      <th>Name</th>
      <th>Positive</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @type_docs.each do |type_doc| %>
      <tr>
        <td><%= type_doc.cod_sii %></td>
        <td><%= type_doc.name %></td>
        <td><%= type_doc.positive %></td>
        <td><%= link_to 'Show', type_doc %></td>
        <td><%= link_to 'Edit', edit_type_doc_path(type_doc) %></td>
        <td><%= link_to 'Destroy', type_doc, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Type doc', new_type_doc_path %>
